service mysqld start
mysql -u root -e "create database if not exists symfony2_db"
mysql -u root -e "grant all on symfony2_db.* to symfony2_user@'172.17.%' identified by 'symfony2_pass' with grant option"
mysql -u root -e "grant replication slave on *.* to 'repl'@'172.17.%' identified by 'repl_pass' with grant option"
service mysqld stop
